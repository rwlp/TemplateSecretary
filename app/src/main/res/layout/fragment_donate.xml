    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="rwlp.templatescretary.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
